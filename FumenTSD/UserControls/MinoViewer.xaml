<UserControl x:Class="FumenTSD.UserControls.MinoViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FumenTSD.UserControls"
             mc:Ignorable="d" Height="Auto" Width="37"
			 x:Name="root" >
	<Grid Background="#FF373737">
		<Grid.RowDefinitions>
			<RowDefinition Height="6"/>
			<RowDefinition Height="Auto"/>
		</Grid.RowDefinitions>
		<TextBlock x:Name="Title" TextWrapping="NoWrap" Text="Name" Foreground="White" FontSize="5" TextAlignment="Center" Height="6" VerticalAlignment="Center" HorizontalAlignment="Center" />
		<UniformGrid  x:Name="MinoContainer" 
		Grid.Row="1" Columns="1">
		</UniformGrid>
	</Grid>
</UserControl>
